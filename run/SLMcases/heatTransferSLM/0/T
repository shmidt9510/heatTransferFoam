FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 273;

boundaryField
{
  bottom
  {
    type            zeroGradient;
  }
  top
  {
    type            fixedValue;
    value           uniform 273;
  }
  left
  {
    type            zeroGradient;
  }
  right
  {
    type            zeroGradient;
  }
  up
  {
    type            zeroGradient;
  }
  down
  {
    type            zeroGradient;
  }
}


