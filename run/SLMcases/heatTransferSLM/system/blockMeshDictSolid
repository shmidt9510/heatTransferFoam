// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

convertToMeters 1e-4;

xmin 0;
xmax 40;
ymin 0;
ymax 5;
zmin 0;
zmax 6;
ymax2 10;
zmid 3;

vertices
(
    ($xmin $ymin $zmid) //0
    ($xmax $ymin $zmid) //1
    ($xmax $ymax $zmid) //2
    ($xmin $ymax $zmid) //3

    ($xmin $ymin $zmax) //4
    ($xmax $ymin $zmax) //5
    ($xmax $ymax $zmax) //6
    ($xmin $ymax $zmax) //7

    ($xmax $ymax2 $zmid) //8
    ($xmin $ymax2 $zmid) //9

    ($xmax $ymax2 $zmax) //10
    ($xmin $ymax2 $zmax) //11

    ($xmin $ymin $zmin) //12
    ($xmax $ymin $zmin) //13

    ($xmin $ymax $zmin) //14
    ($xmax $ymax $zmin) //15

    ($xmin $ymax2 $zmin) //16
    ($xmax $ymax2 $zmin) //17

);

blocks
(
    hex (0 1 2 3 4 5 6 7) (80 30 15) simpleGrading (1 1 2)
    hex (3 2 8 9 7 6 10 11) (80 30 15) simpleGrading (1 1 2)
    hex (12 13 15 14 0 1 2 3) (80 30 20) simpleGrading (1 1 1)
    hex (14 15 17 16 3 2 8 9) (80 30 20) simpleGrading (1 1 1)

);
