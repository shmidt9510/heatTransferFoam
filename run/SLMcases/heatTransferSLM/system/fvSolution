solvers
{
    T
    {
        solver          PCG;
        preconditioner  DIC;
        tolerance       1e-06;
        smoother        DICGaussSeidel;
        relTol          0;
    }

    he
    {
        solver          PCG;
        preconditioner  diagonal;
        tolerance       1e-05;
        relTol          0;
    }


    stressAnalysis
    {
        compactNormalStress yes;
        nCorrectors     1;
        DU               1e-06;
    }


// DU
