initialPorosity initialPorosity [ 0 0 0 0 0 0 0 ] 0.4;

absorptivity    absorptivity [ 0 0 0 0 0 0 0 ] 0.3;

powderDepth     powderDepth [ 0 1 0 0 0 0 0 ] 2e-05;

airDepth        airDepth [ 0 1 0 0 0 0 0 ] 0;

airDensity      airDensity [ 1 -3 0 0 0 0 0 ] 0.1;

airConductivity airConductivity [ 1 1 -3 -1 0 0 0 ] 0.0018;

airCapacity     airCapacity [ 0 2 -2 -1 0 0 0 ] 520;


