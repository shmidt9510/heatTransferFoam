 // The three components ofthe displacement in the mechanical equations are
 //  solved coupled usinga DILU preconditioned, stabilized bi-conjugate gradient solver.
 // Multi-scale computational modeling of residual stress in selective lasermelting with uncertainty quantification
    DU
    {
        solver          PBiCGStab; //PCG
        preconditioner  DIC; //DIC

        maxIter         3000;
        tolerance       1e-05;
        relTol          0.1;
        // minIter 1;
        smoother        DICGaussSeidel;
        // cacheAgglomeration true;
        // nCellsInCoarsestLevel 15;
        // agglomerator    faceAreaPair;
        // mergeLevels     3;
    }

    pcorr
    {
        solver           BiCGStab;
        preconditioner   DILU;

        minIter          0;
        maxIter          1000;
        tolerance        1e-02;
        relTol           0;
    };

    p
    {
        solver           CG;
        preconditioner   Cholesky;

        minIter          0;
        maxIter          1000;
        tolerance        1e-07;
        relTol           0.0;
    };

    pFinal
    {
        solver           CG;
        preconditioner   Cholesky;

        minIter          0;
        maxIter          1000;
        tolerance        1e-07;
        relTol           0;
    };

    Umesh
    {
        solver            BiCGStab;
        preconditioner    DILU;

        minIter          0;
        maxIter          1000;
        tolerance        1e-07;
        relTol           0;
    };
}


